orig  = icms.origem
CSOSN = icms.codigo_cst

- if icms.aliquota.to_f > 0.0
	modBC    = icms.modalidade_base_calculo
	- if icms.reducao_base_calculo.to_f > 0.0
		pRedBC   = nf_xml_value_percent  icms.reducao_base_calculo
	vBC      = nf_xml_value_monetary icms.total_base_calculo
	pICMS    = nf_xml_value_percent  icms.aliquota
	vICMS    = nf_xml_value_monetary icms.total

/################## ICMS ST ##################\
- if icms.aliquota_st.to_f > 0.0
	modBCST  = icms.modalidade_base_calculo_st
	- if icms.mva_st.to_f > 0.0
		pMVAST   = nf_xml_value_percent  icms.mva_st
	- if icms.reducao_base_calculo_st.to_f > 0.0
		pRedBCST = nf_xml_value_percent  icms.reducao_base_calculo_st
	vBCST    = nf_xml_value_monetary icms.total_base_calculo_st
	pICMSST  = nf_xml_value_percent  icms.aliquota_st
	vICMSST  = nf_xml_value_monetary icms.total_st

- if icms.aliquota_credito_sn.to_f > 0.0
	pCredSN     = nf_xml_value_percent  icms.aliquota_credito_sn
	vCredICMSSN = nf_xml_value_monetary icms.total_credito_sn